<?xml version='1.0' encoding='utf-8'?>
<widget id="com.earthoracles.celtmistic" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>The Talismans</name>
    <description>
        An attempt to build an app with IAP.
    </description>
    <author email="admin@koalatea-software.com" href="http://koalateasoftware.com">
        Koala Tea Software
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="android-minSdkVersion" value="28" />
    <preference name="android-targetSdkVersion" value="28" />
</widget>
